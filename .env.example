# NWU Protocol Environment Variables
# Copy this file to .env.local and fill in your values

# Database
MONGODB_URI=mongodb://localhost:27017/nwu_db
DATABASE_URL=postgresql://user:password@localhost:5432/nwu_db
REDIS_URL=redis://localhost:6379

# IPFS
IPFS_API_URL=http://localhost:5001
IPFS_GATEWAY_URL=https://gateway.ipfs.io

# API Keys
OPENAI_API_KEY=sk-
ANTHROPIC_API_KEY=

# Blockchain
ETHEREUM_RPC_URL=https://sepolia-rpc.com
PRIVATE_KEY=

# Environment
NODE_ENV=development
DEBUG=true

# Ports
API_PORT=3000
DASHBOARD_PORT=3001
